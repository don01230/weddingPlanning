    </script>
    <script type="text/javascript">
      function initialize() {
        var mapOptions = {
          center: { lat: 22.25, lng: 114.1667},
          zoom: 8
        };
        var map = new google.maps.Map(
            document.getElementById('map-canvas'),
            mapOptions);
      }
      google.maps.event.addDomListener(
          window, 'load', initialize);
    </script>
